{
  "AutoPromo Basic Campaign": {
    "prefix": "aetherpost-basic",
    "body": [
      "name: \"${1:my-app}\"",
      "concept: \"${2:Brief description of your app}\"",
      "platforms: [${3:twitter}]",
      "content:",
      "  style: \"${4|casual,professional,technical,humorous|}\"",
      "  action: \"${5:Learn more!}\""
    ],
    "description": "Basic AutoPromo campaign configuration"
  },
  
  "AutoPromo Complete Campaign": {
    "prefix": "aetherpost-complete",
    "body": [
      "name: \"${1:my-app}\"",
      "concept: \"${2:Detailed description of your application}\"",
      "url: \"${3:https://myapp.com}\"",
      "",
      "platforms:",
      "  - ${4|twitter,bluesky,mastodon,linkedin,dev_to|}",
      "  - ${5|twitter,bluesky,mastodon,linkedin,dev_to|}",
      "",
      "content:",
      "  style: \"${6|casual,professional,technical,humorous|}\"",
      "  action: \"${7:Try it free!}\"",
      "  max_length: ${8:280}",
      "  hashtags:",
      "    - \"${9:hashtag1}\"",
      "    - \"${10:hashtag2}\"",
      "",
      "schedule:",
      "  type: \"${11|immediate,delayed,recurring|}\"",
      "  ${12:# time: \"2025-06-14 10:00\"}",
      "",
      "image: \"${13|generate,auto,none|}\"",
      "analytics: ${14|true,false|}"
    ],
    "description": "Complete AutoPromo campaign with all options"
  },
  
  "AutoPromo A/B Test": {
    "prefix": "aetherpost-abtest",
    "body": [
      "name: \"${1:ab-test-campaign}\"",
      "concept: \"${2:Your app concept}\"",
      "platforms: [${3:twitter}]",
      "",
      "experiments:",
      "  ab_test:",
      "    enabled: true",
      "    variants:",
      "      - action: \"${4:Try it now!}\"",
      "        style: \"casual\"",
      "      - action: \"${5:Start free trial!}\"",
      "        style: \"professional\"",
      "    metric: \"${6|engagement_rate,clicks,likes,shares|}\"",
      "    duration: \"${7:7}_days\""
    ],
    "description": "AutoPromo A/B testing configuration"
  },
  
  "AutoPromo Scheduled Post": {
    "prefix": "aetherpost-schedule",
    "body": [
      "name: \"${1:scheduled-campaign}\"",
      "concept: \"${2:Your scheduled announcement}\"",
      "platforms: [${3:twitter}]",
      "",
      "schedule:",
      "  type: \"${4|delayed,recurring|}\"",
      "  time: \"${5:2025-06-14 10:00}\"",
      "  ${6:frequency: \"weekly\"}",
      "  ${7:timezone: \"UTC\"}"
    ],
    "description": "Scheduled AutoPromo campaign"
  },
  
  "AutoPromo Multi-Platform": {
    "prefix": "aetherpost-multi",
    "body": [
      "name: \"${1:multi-platform-campaign}\"",
      "concept: \"${2:Cross-platform announcement}\"",
      "",
      "platforms:",
      "  - twitter",
      "  - bluesky", 
      "  - linkedin",
      "",
      "content:",
      "  style: \"professional\"",
      "  action: \"${3:Join the conversation!}\"",
      "  platform_specific:",
      "    twitter:",
      "      style: \"casual\"",
      "      max_length: 280",
      "    linkedin:",
      "      style: \"professional\"",
      "      max_length: 1300"
    ],
    "description": "Multi-platform campaign with platform-specific settings"
  },
  
  "AutoPromo Quick Post": {
    "prefix": "aetherpost-quick",
    "body": [
      "# Quick post command:",
      "# aetherpost now \"${1:Your message here}\" --to ${2:twitter} --style ${3:casual}"
    ],
    "description": "Quick post command template"
  }
}